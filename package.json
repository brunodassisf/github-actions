{
  "name": "github-actions",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clen:next": "rm -rf .next",
    "build": "yarn clen:next && yarn lint:fix && next build && next export && mv ./out ./build && yarn clen:next",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy:staging": "yarn build && vercel ./build --yes --token=$VERCEL_TOKEN",
    "deploy:prod": "yarn deploy:staging --prod",
    "deploy:preview": "node ./scripts/deploy-preview.mjs --trace-warnings"
  },
  "dependencies": {
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "next": "13.4.10",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0"
  }
}
